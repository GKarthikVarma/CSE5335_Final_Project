/*Signup php*/
.signup-form {
    width: 400px;
    margin: 0 auto;
    padding-top: 30px;
}

.signup-form input.text-input{
    width: 90%;
    height: 40px;
    padding: 0px 5%;
    border: none;
    background-color: #fff;
    font-family: arial;
    font-size: 16px;
    color: #111;
    line-height: 40px;
    margin-bottom: 4px;
}

.signup-form button {
    display: block;
    width: 30%;
    height: 40px;
    border: none;
    margin: 0 auto;
    color: white;
    background-color: #F38130;
    font-family: arial;
    font-size: 16px;
    color: #fff;
    cursor: pointer;
}

.signup-form button:hover {
    background-color: #df7813;
}


/*form type A*/

