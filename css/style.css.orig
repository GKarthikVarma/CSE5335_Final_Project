/* NAV BAR STYLES */
@media only screen and (max-width: 480px) {
  header nav{
      width: 100%;
      height: 60px;
      background-color: #0E66B0;
  }

  header nav ul{
      float: left;
  }

  header nav ul li{
      float: left;
      list-style: none;
  }

  header nav ul li a{
      font-family: arial;
      font-size: 16px;
      color: #111;
      line-height: 63px;
  }


header .nav-login {
  float: right;
}
  header .nav-login form{
      padding-top: 15px;
      float: left;
  }

  header .nav-login form input{
      float: left;
      width: 140px;
      height: 30px;
      padding: 0px 10px;
      margin-right: 10px;
      border: none;
      background-color: #ffffff;
      font-family: arial;
      font-size: 14px;
      color: #111;
      line-height: 30px;
  }

  header .nav-login form button{
      float: left;
      width: 140px;
      height: 30px;
      margin-right: 10px;
      border: none;
      background-color: #F38130;
      font-family: arial;
      font-size: 14px;
      color: #fff;
      cursor: pointer;

  }

  header .nav-login form button:hover{
      background-color: #df7813;
  }

  header .nav-login a{
      display: block;
      width: 60px;
      height: 60px;
      border: none;
      float: left;
      font-family: arial;
      font-size: 16px;
      color: #F38130;
      line-height: 63px;
      cursor: pointer;
  }

  header .navbar {
    float: left;
  }

  header .navbar li {
    margin: 0 10px;
  }

  header .navbar a{
    color: #F38130;
    font-weight: bold;
    font-size: 12px
  }

  header .title {
    display: none;
  }
}


